%%[
  var @salutation
  set @salutation = ProperCase(FirstName)
  if Empty(@salutation) then
    set @salutation = "Hey there"
  Endif

  /* Product Information */
  var @productName, @productImageURL, @productURL, @productHandle
  
  set @productName = "Camiseta MÃ©xico"
  set @productImageURL = "https://cdn.shopify.com/s/files/1/2399/8125/products/mexico.jpeg?v=1506554147"
  set @productHandle = "mexico-national-team-federation-t-shirt-green"
  set @productURL = Concat("https://demo-fultbolshop.myshopify.com/products/", @productHandle)
  
  var @abandonedCartDetails, @cartDetail
  set @abandonedCartDetails = LookupOrderedRows("AbandonBasketDetails", 1, "UnitPrice DESC", "BasketID", BasketID)
  IF RowCount(@abandonedCartDetails) > 0 THEN
     set @cartDetail = ROW(@abandonedCartDetails,1)
     set @productID = Field(@cartDetail,"PID") 
     set @products = LookupRows("ProductCatalog", "PID", @productID)
     IF RowCount(@products) > 0 THEN
         set @productRow = Row(@products,1)
         set @productName = Field(@productRow,"Name")
         set @productImageUrl = Field(@productRow,"ImageURL")
         set @productHandle = Field(@productRow,"Handle")
         set @productURL = Concat("https://demo-fultbolshop.myshopify.com/products/", @productHandle)
     ENDIF
  ENDIF
]%%
